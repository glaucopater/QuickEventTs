{
  "expo": {
    "name": "QuickEventTs",
    "slug": "QuickEventTs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ],
      "package": "com.flagg.QuickEventTs"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/splash.png",
          "color": "#9977ff",
          "sounds": [
            "./assets/thunder124463.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d209c9d2-189c-4018-aa64-71ed20a93b63"
      }
    }
  }
}
